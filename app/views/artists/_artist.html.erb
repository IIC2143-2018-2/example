<tr>
  <td><%= artist.name %></td>
  <td><%= artist.birth_year %></td>
  <td><%= artist.first_album_year %></td>
  <td></td>
  <td><%= link_to 'Show', artist, class: 'btn btn-light' %></td>
  <td><%= link_to 'Edit', edit_artist_path(artist), class: 'btn btn-light' %></td>
  <td><%= link_to 'Delete', artist, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger' %></td>
</tr>
